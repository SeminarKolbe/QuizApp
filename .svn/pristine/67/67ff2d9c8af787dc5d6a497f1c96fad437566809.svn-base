public class Frage {
    public String question;
    public String answer;
    public String getQuestion() {
        return question;
    }

    public String getAnswer() {
        return answer;
    }
    
    public Frage(String frage, String antwort){
        this.question=frage;
        this.answer=antwort;
    }
}
