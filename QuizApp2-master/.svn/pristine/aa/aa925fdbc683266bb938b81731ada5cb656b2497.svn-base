<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>JSP Page</title>
    </head>
    <body>
        
        <% int wrong = (int)request.getAttribute("wrong");%>
        <% int right = (int)request.getAttribute("right");%>
        <h1>Endergebnis</h1>
        <%
            out.println("Du hast soviele Fragen richtig bewantwortet: "+right);
            out.println("Du hast soviele Fragen falsch bewantwortet: "+wrong);
          %>  
    </body>
</html>
